<aura:component >
    <aura:attribute name="name" type="String" default="no_name" access="public"/>
    <aura:attribute name="Age"  type="integer" default="0" access="public"/>
     <aura:attribute name="Rno"  type="integer" default="0" access="public"/>
    <aura:attribute name="stu_list" type="List" access="public"/>
    <lightning:input type="String" name="input1" label="Enter a name" value="{!v.name}"/><br/>
    <lightning:input type="number" name="input2" label="Enter a age" value="{!v.Age}"/><br/>
    <lightning:button label="Check" value="Check" onclick="{!c.doClick}"/><br/>
    
    
    Welcome {!v.name}.You are {!v.Age} years old.
</aura:component>